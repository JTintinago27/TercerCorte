public class EjecutarClaseRectangulo {
    public static void main(String[] args) {
        
        ClaseRectangulo rectanguloa = new ClaseRectangulo(10, 5);
        ClaseRectangulo rectangulob = new ClaseRectangulo(20, 10);

        System.out.println("Rectángulo 1:");
        System.out.println("Altura: " + rectanguloa.getBase());
        System.out.println("Base: " + rectanguloa.getBase());
        System.out.println("Área: " + rectanguloa.calcularArea());
        System.out.println("Perímetro: " + rectanguloa.calcularPerimetro());

        System.out.println("\nRectángulo 2:");
        System.out.println("Altura: " + rectangulob.getBase());
        System.out.println("Base: " + rectangulob.getBase());
        System.out.println("Área: " + rectangulob.calcularArea());
        System.out.println("Perímetro: " + rectangulob.calcularPerimetro());
    }
 }
